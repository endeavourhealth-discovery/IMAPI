{
  "query" : {
    "name" : "health care organisations in EW1",
    "description" : "name and address of organisations with post code starting with EW1",
    "select" : {
      "entityType" : {
        "name" : "Organisation",
        "@id" : "http://endhealth.info/im#Organisation"
      },
      "property" : [ {
        "@id" : "http://www.w3.org/2000/01/rdf-schema#label",
        "alias" : "name"
      }, {
        "@id" : "http://endhealth.info/im#address",
        "select" : {
          "property" : [ {
            "@id" : "http://endhealth.info/im#postCode",
            "alias" : "postCode"
          } ]
        }
      } ],
      "match" : [ {
        "property" : [ {
          "@id" : "http://endhealth.info/im#address",
          "alias" : "address",
          "match" : {
            "property" : [ {
              "@id" : "http://endhealth.info/im#postCode",
              "value" : {
                "valueData" : "E1W 3DP"
              }
            } ]
          }
        } ]
      } ]
    },
    "usePrefixes" : true
  }
}