PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX im: <http://endhealth.info/im#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX sn: <http://snomed.info/sct#>
PREFIX sh: <http://www.w3.org/ns/shacl#>

PREFIX con-inst: <http://www.ontotext.com/connectors/lucene/instance#>
PREFIX con: <http://www.ontotext.com/connectors/lucene#>
SELECT distinct ?entity  ?p4 ?p5
WHERE {[] a con-inst:im_fts;
       con:query "label:(causative*)" ;
       con:entities ?entity.
?entity <http://endhealth.info/im#isA> ?o1.
?o1 <http://www.w3.org/2000/01/rdf-schema#domain> ?o2.
?o2 ^im:isA ?subtype3.
Filter (?subtype3=<http://snomed.info/sct#674814021000119106>)
?entity im:status im:Active.
 OPTIONAL { ?entity <http://endhealth.info/im#code> ?p4.}
 OPTIONAL { ?entity <http://www.w3.org/2000/01/rdf-schema#label> ?p5.}
}
ORDER BY DESC(strstarts(lcase(?p5),"causative")) ASC(strlen(?p5))
