@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix im: <http://endhealth.info/im#>.
@prefix fno:     <https://w3id.org/function/ontology#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix fnml:     <http://semweb.mmlab.be/ns/fnml#> .
@prefix lib: <http://example.com/library#> .

im:valueParam a fno:Parameter ;
  fno:name "input value" ;
  rdfs:label "input value" ;
  fno:type xsd:string ;
  fno:predicate im:valueParameter ;
  fno:required "true"^^xsd:boolean .

im:valueParam2 a fno:Parameter ;
  fno:name "input value 2" ;
  rdfs:label "input value 2" ;
  fno:type xsd:string ;
  fno:predicate im:valueParameter2 ;
  fno:required "true"^^xsd:boolean .

im:stringOut a fno:Output ;
  fno:name "output string" ;
  rdfs:label "output string" ;
  fno:type xsd:string ;
  fno:predicate im:stringOutput .

im:toUpperCaseEndeavour a fno:Function ;
  fno:name "to Uppercase" ;
  rdfs:label "to Uppercase" ;
  dcterms:description "Returns the input with all letters in upper case." ;
  fno:expects ( im:valueParam ) ;
  fno:returns ( im:stringOut ) ;
  lib:providedBy [
    lib:localLibrary "src/test/resources/MappingFunctions.jar";
    lib:class "TestFunc";
    lib:method "toUpperEndeavour"
  ].

im:generateIri a fno:Function ;
  fno:name "generate iri" ;
  rdfs:label "generate iri" ;
  dcterms:description "Returns an iri from the name property." ;
  fno:expects ( im:valueParam im:valueParam2 ) ;
  fno:returns ( im:stringOut ) ;
  lib:providedBy [
    lib:localLibrary "src/test/resources/MappingFunctions.jar";
    lib:class "TestFunc";
    lib:method "generateIri"
  ].

im:getType a fno:Function ;
  fno:name "get type" ;
  rdfs:label "get type" ;
  dcterms:description "Returns a type" ;
  fno:expects ( im:valueParam ) ;
  fno:returns ( im:stringOut ) ;
  lib:providedBy [
    lib:localLibrary "src/test/resources/MappingFunctions.jar";
    lib:class "TestFunc";
    lib:method "getType"
  ].
