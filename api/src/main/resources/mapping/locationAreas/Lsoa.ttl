@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix im: <http://endhealth.info/im#>.
@base <http://example.com/ns#>.

<#LSOAV2> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "location_areas.tsv-1.csv" ;
    rml:referenceFormulation ql:CSV
  ];
  rr:subjectMap [
    rr:template "http://endhealth.info/ref#Lsoa_2011{lsoa 2011 code}";
    rr:class im:Lsoa
  ];

  rr:predicateObjectMap [
    rr:predicate im:code;
    rr:objectMap [
      rml:reference "lsoa 2011 code";
      rr:datatype xsd:String
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "lsoa name"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate im:postcode;
    rr:objectMap [
      rml:reference "postcode"
    ]
  ].