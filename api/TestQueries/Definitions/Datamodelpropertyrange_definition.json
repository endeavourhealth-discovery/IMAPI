{
  "@context" : {
    "im" : "http://endhealth.info/im#",
    "rdf" : "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "owl" : "http://www.w3.org/2002/07/owl#",
    "sh" : "http://www.w3.org/ns/shacl#",
    "xsd" : "http://www.w3.org/2001/XMLSchema#",
    "rdfs" : "http://www.w3.org/2000/01/rdf-schema#",
    "sn" : "http://snomed.info/sct#"
  },
  "argument" : [ {
    "parameter" : "myDataModel",
    "valueIri" : {
      "@id" : "im:Observation"
    }
  }, {
    "parameter" : "myProperty",
    "valueIri" : {
      "@id" : "im:concept"
    }
  } ],
  "query" : {
    "name" : "Data model property range",
    "description" : "get node, class or datatype value (range)  of property objects for specific data model and property",
    "match" : [ {
      "bool" : "and",
      "instanceOf" : [ {
        "parameter" : "myDataModel"
      } ],
      "where" : [ {
        "@id" : "sh:path",
        "nodeRef" : "shaclProperty",
        "is" : [ {
          "parameter" : "myProperty"
        } ]
      }, {
        "bool" : "or",
        "where" : [ {
          "@id" : "sh:class",
          "nodeRef" : "shaclProperty",
          "valueVariable" : "propType"
        }, {
          "@id" : "sh:node",
          "nodeRef" : "shaclProperty",
          "valueVariable" : "propType"
        }, {
          "@id" : "sh:datatype",
          "nodeRef" : "shaclProperty",
          "valueVariable" : "propType"
        } ]
      } ],
      "path" : [ {
        "@id" : "sh:property",
        "variable" : "shaclProperty"
      } ],
      "return" : {
        "property" : [ {
          "@id" : "rdfs:label",
          "valueRef" : "propType"
        } ]
      }
    } ]
  }
}