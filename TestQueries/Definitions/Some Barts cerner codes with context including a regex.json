            },
            {
              "@id": "http://endhealth.info/im#sourceHeading"
            },
            {
              "@id": "http://endhealth.info/im#sourceSystem"
            },
            {
              "@id": "http://endhealth.info/im#sourceTable"
            },
            {
              "@id": "http://endhealth.info/im#sourceField"
            },
            {
              "@id": "http://endhealth.info/im#sourceSchema"
            }
          ]
        }
      }
    ],
    "match": [
      {
        "property": [
          {
            "@id": "http://endhealth.info/im#sourceContext",
            "match": {
              "property": [
                {
                  "@id": "http://endhealth.info/im#sourceRegex"
