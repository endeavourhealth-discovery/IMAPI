{
  "context": {
    "im": "http://endhealth.info/im#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "sn": "http://snomed.info/sct#"
  },
  "argument": [
    {
      "parameter": "dataModel",
      "valueIri": {
        "iri": "im:Patient"
      }
    },
    {
      "parameter": "property",
      "valueIri": {
        "iri": "im:age"
      }
    }
  ],
  "query": {
    "name": "Query - shacl property predicates for a property in a data model",
    "description": "Select the predicates and values and labels of the values for a given data mode and property",
    "match": [
      {
        "parameter": "$dataModel",
        "property": [
          {
            "iri": "sh:property",
            "match": {
              "variable": "shaclProperty",
              "property": [
                {
                  "iri": "sh:path",
                  "in": [
                    {
                      "parameter": "$property"
                    }
                  ]
                }
              ]
            }
          }
        ]
      }
    ],
    "return": [
      {
        "nodeRef": "shaclProperty",
        "property": [
          {
            "iri": "sh:class",
            "return": {
              "property": [
                {
                  "iri": "rdfs:label"
                }
              ]
            }
          },
          {
            "iri": "sh:node",
            "return": {
              "property": [
                {
                  "iri": "rdfs:label"
                }
              ]
            }
          },
          {
            "iri": "sh:datatype",
            "return": {
              "property": [
                {
                  "iri": "rdfs:label"
                }
              ]
            }
          },
          {
            "iri": "sh:maxCount"
          },
          {
            "iri": "sh:minCount"
          }
        ]
      }
    ]
  }
}