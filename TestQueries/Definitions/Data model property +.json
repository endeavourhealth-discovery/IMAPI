{
  "name": "Data model property range",
  "description": "get node, class or datatype value (range)  of property objects for specific data model and property",
  "match": [
    {
      "path": {
        "iri": "http://www.w3.org/ns/shacl#property",
        "node": {
          "variable": "shaclProperty"
        }
      },
      "where": [
        {
          "iri": "http://www.w3.org/ns/shacl#path",
          "in": [
            {
              "parameter": "myProperty"
            }
          ]
        }
      ],
      "parameter": "myDataModel"
    }
  ],
  "return": [
    {
      "nodeRef": "shaclProperty",
      "property": [
        {
          "node": {
            "property": [
              {
                "iri": "http://www.w3.org/2000/01/rdf-schema#label"
              }
            ]
          },
          "iri": "http://www.w3.org/ns/shacl#class"
        },
        {
          "node": {
            "property": [
              {
                "iri": "http://www.w3.org/2000/01/rdf-schema#label"
              }
            ]
          },
          "iri": "http://www.w3.org/ns/shacl#node"
        },
        {
          "node": {
            "property": [
              {
                "iri": "http://www.w3.org/2000/01/rdf-schema#label"
              }
            ]
          },
          "iri": "http://www.w3.org/ns/shacl#datatype"
        }
      ]
    }
  ]
}