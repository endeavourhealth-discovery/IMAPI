{
  "context": {
    "im": "http://endhealth.info/im#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "sn": "http://snomed.info/sct#"
  },
  "textSearch": "Diabetes",
  "query": {
    "name": "test search with Diabetes",
    "match": [
      {
        "where": [
          {
            "iri": "im:binding",
            "match": {
              "where": [
                {
                  "iri": "sh:path",
                  "is": [
                    {
                      "iri": "im:concept"
                    }
                  ]
                },
                {
                  "iri": "sh:node",
                  "is": [
                    {
                      "iri": "im:Condition"
                    }
                  ]
                }
              ]
            }
          },
          {
            "iri": "rdf:type",
            "is": [
              {
                "iri": "im:Concept"
              },
              {
                "iri": "im:ConceptSet"
              }
            ]
          },
          {
            "iri": "im:status",
            "is": [
              {
                "iri": "im:Active"
              }
            ]
          },
          {
            "iri": "im:scheme",
            "is": [
              {
                "iri": "im:"
              },
              {
                "iri": "sn:"
              }
            ]
          }
        ]
      }
    ]
  }
}