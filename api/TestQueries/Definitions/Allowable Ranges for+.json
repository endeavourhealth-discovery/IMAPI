{
  "name": "Allowable Ranges for a property and super properties",
  "match": [
    {
      "where": [
        {
          "@id": "http://www.w3.org/2000/01/rdf-schema#range",
          "in": [
            {
              "parameter": "this"
            }
          ],
          "ancestorsOf": true,
          "inverse": true
        }
      ]
    }
  ],
  "activeOnly": true,
  "return": [
    {
      "property": [
        {
          "@id": "http://endhealth.info/im#code"
        },
        {
          "@id": "http://www.w3.org/2000/01/rdf-schema#label"
        }
      ]
    }
  ]
}