{
  "context": {
    "im": "http://endhealth.info/im#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "sn": "http://snomed.info/sct#"
  },
  "argument": [
    {
      "parameter": "dataModel",
      "valueIri": {
        "iri": "http://endhealth.info/im#Patient"
      }
    },
    {
      "parameter": "property",
      "valueIri": {
        "iri": "http://endhealth.info/im#age"
      }
    }
  ],
  "query": {
    "name": "Query - property from a data model",
    "match": [
      {
        "path": {
          "iri": "http://www.w3.org/ns/shacl#property"
        },
        "where": [
          {
            "in": [
              {
                "parameter": "$property"
              }
            ],
            "iri": "http://www.w3.org/ns/shacl#path"
          }
        ],
        "parameter": "$dataModel"
      }
    ],
    "select": [
      {
        "iri": "http://www.w3.org/ns/shacl#property",
        "select": [
          {
            "iri": "http://www.w3.org/ns/shacl#path",
            "select": [
              {
                "iri": "http://www.w3.org/2000/01/rdf-schema#label"
              }
            ]
          },
          {
            "iri": "http://www.w3.org/ns/shacl#class",
            "select": [
              {
                "iri": "http://www.w3.org/2000/01/rdf-schema#label"
              }
            ]
          },
          {
            "iri": "http://www.w3.org/ns/shacl#node",
            "select": [
              {
                "iri": "http://www.w3.org/2000/01/rdf-schema#label"
              }
            ]
          },
          {
            "iri": "http://www.w3.org/ns/shacl#datatype",
            "select": [
              {
                "iri": "http://www.w3.org/2000/01/rdf-schema#label"
              }
            ]
          },
          {
            "iri": "http://www.w3.org/ns/shacl#maxCount"
          },
          {
            "iri": "http://www.w3.org/ns/shacl#minCount"
          }
        ]
      }
    ],
    "activeOnly": true
  }
}