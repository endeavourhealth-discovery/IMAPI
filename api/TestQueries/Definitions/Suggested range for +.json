{
  "name": "Suggested range for a data property",
  "description": "get datatype values (ranges)  of property objects that have 4this as their path",
  "match": [
    {
      "path": {
        "@id": "http://www.w3.org/ns/shacl#datatype",
        "node": {
          "variable": "range"
        }
      }
    }
  ],
  "return": [
    {
      "nodeRef": "range",
      "property": [
        {
          "@id": "http://www.w3.org/2000/01/rdf-schema#label"
        }
      ]
    }
  ]
}